{
    "version": 2,
    "builds": [
      {
        "src": "mysite1/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      
      
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "djangoprojectname/wsgi.py"
        },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "mysite1/wsgi.py"
      }
    ]
  }
